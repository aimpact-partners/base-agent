{
	"name": "@aimpact/base-agent",
	"version": "1.0.0",
	"title": "Base Agents package",
	"description": "AImpact base agent",
	"repository": "https://github.com/aimpact-partners/base-agent",
	"license": "ISC",
	"template": "template/template.json",
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"modules": {
		"path": "modules"
	},
	"deployment": {
		"distributions": [
			{
				"name": "node-local",
				"environment": "local",
				"platform": "node",
				"maps": "external",
				"ports": {
					"bundles": 3080,
					"inspect": 3081
				}
			},
			{
				"name": "node",
				"environment": "development",
				"platform": "node",
				"maps": "external",
				"ports": {
					"bundles": 3083,
					"inspect": 3084
				}
			},
			{
				"name": "node-tsc",
				"environment": "development",
				"platform": "node",
				"maps": "external",
				"ts": {
					"compiler": "tsc"
				},
				"ports": {
					"bundles": 3086
				}
			}
		]
	},
	"dependencies": {
		"@beyond-js/kernel": "~0.1.9",
		"@beyond-js/local": "~0.1.3",
		"@aimpact/agents-client": "~0.0.15",
		"socket.io-client": "^4.5.4",
		"express": "^4.18.2",
		"openai": "~4.2.0",
		"dotenv": "^16.0.3"
	},
	"devDependencies": {
		"@beyond-js/bee": "^0.0.6",
		"@types/express": "^4.17.17",
		"@types/node": "~20.6.0"
	}
}
